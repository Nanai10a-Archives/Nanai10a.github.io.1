{
  "name": "Nanai10a.github.io",
  "version": "0.1.0",
  "repository": "https://github.com/Nanai10a/Nanai10a.github.io",
  "author": "Nanai10a <Nanai10a@tutanota.com>",
  "license": "MIT",
  "scripts": {
    "clear:next": "rimraf .next",
    "clear:dist": "rimraf dist",
    "clear": "run-s clear:next clear:dist",
    "etsc:dev": "esbuild ./src/ --tsconfig=./src/tsconfig.json --outdir=dist --platform=node --target=esnext --format=cjs --sourcemap",
    "etsc:min": "esbuild ./src/ --tsconfig=./src/tsconfig.json --outdir=dist --platform=node --target=esnext --format=cjs --sourcemap --minify",
    "next:build": "next build",
    "run:dev": "env-cmd -e dev node dist/index.js",
    "run:prod": "env-cmd -e prod node dist/index.js",
    "dev": "tsc-watch -p src --onSuccess 'yarn run-s etsc:dev run:dev'",
    "prod": "run-s clear etsc:min next:build run:prod"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "fastify": "^3.9.2",
    "next": "^10.0.5",
    "pino-pretty": "^4.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/next": "^9.0.0",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.8.31",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  }
}
